// ConsoleApplication9.cpp : 定义 DLL 应用程序的导出函数。
//

#include "stdafx.h"
#include <iostream>

using namespace std;

void printstar(void)
{
	cout<<"...."<<endl;
}

void print_message(void)
{
cout<<"..."<<endl;
}

int main(void)
{
	printstar();
	print_message();
	printstar();
	return 0;
}

