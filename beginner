2.
//先定义一个函数，再输入数值

#include "stdafx.h"
#include <iostream>

using namespace std;

int max(int x,int y)
{
int z;
if(x>y)z=x;
else z=y;
return z;
}

int main()
{
int a,b,c;
cin>>a>>b;
c=max(a,b);
cout<<"最大的是"<<c';
return 0;
}

